<div data-section-id="{{ section.id }}">
    {% for block in section.blocks %}
        <div {{ block.shopify_attributes }}>
            {% case block.type %}

              {% when 'page-hero-legal' %}
              {% include 'page-hero-legal' %}

              {% when 'page-content-container' %}
              {% include 'page-content-container' %}


          {% endcase %}
        </div>
    {% endfor %}
</div>

{% schema %}
  {
    "blocks": [
      
      {
        "type": "page-hero-legal",
        "name": "Legal Page Hero",
        "settings": [
          {
            "id": "legal-hero-background",
            "type": "image_picker",
            "label": "Hero Background"
          },
          {
            "id": "legal-hero-nav-links",
            "type": "link_list",
            "label": "Legal Nav Links"
          }
        ]
      },
      {
        "type": "page-content-container",
        "name": "Page Content",
        "settings": [

        ]
      }
    ]
  }
{% endschema %}