<div data-section-id="{{ section.id }}">
  {% for block in section.blocks %}
  <div {{ block.shopify_attributes }}>
    {% case block.type %}

      {% when 'article-main' %}
      {% include 'article-main' %}

    {% endcase %}
  </div>
  {% endfor %}
</div>

{% schema %}
{
  "blocks": [
    {
      "type": "article-main",
      "name": "Article Content",
      "settings": []
    }
  ]
}
{% endschema %}